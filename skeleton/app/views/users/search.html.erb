<h1>Search!</h1>

<nav class="users-search">
  <label for="username">Username</label>
  <input type="text" name="query" id="username" value="">

  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <a href="<%= user_url(user) %>"><%= user.username %></a>
        <%= render 'follows/form', user: user %>
      </li>
    <% end %>
  </ul>
</nav>


